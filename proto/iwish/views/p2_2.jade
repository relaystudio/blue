extends layout

block content
	.scrollArea
	img(src="/img/new/p_-02.png")
	.scroll.sharedList
		-for(var i=0;i<11;i++)
			- var imgpath = '/img/items/' + i + '.jpg'
			-if(i == 5)
				.dragItem.myItem(style='clear:both;') 
					img.flex(src=imgpath)
			-else
				.dragItem.myItem
					img.flex(src=imgpath)
					.inner
						a(href='#') Add to cart | 
						a(href='#') Add to list
	.feedItem.listOpen(style='position:relative')
		img(src='/img/new/mylists.png')
		.listDrawer(style='top:211px')
			.scroll.sharedList(style="position: absolute;top: 0; left: 0;")
				-for(var i=0;i<11;i++)
					- var imgpath = '/img/items/' + i + '.jpg'
					-if(i == 5)
						.dragItem.myItem(style='clear:both;') 
							img.flex(src=imgpath)
					-else
						.dragItem.myItem
							img.flex(src=imgpath)
			img(src='/img/new/flag.png', style='position:absolute; left: 0; bottom: 0')



block bar
	.bar.proto2
		.content
			.dropSpot.scroll
				-var listNames = ['John\'s Bday', 'Running gear', 'New den', 'Webdev stuff', 'XKCD only', 'Toronto', 'Other stuff','iPhone','Cooking books']
				-for(var i=0;i<3;i++)
					.items
						a(href='/proto/2')= listNames[i]

block template